{
  "expo": {
    "name": "Weather Now",
    "slug": "weather-now",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#004766"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Aplicação para verificação climática utilizando a API do AccuWeather. ",
    "githubUrl": "https://github.com/alvesrafa/weather-now/tree/master/mobile"
  }
}
